            <hc:SimplePanel Margin="10,10,10,10" Background="#FF101010">
                <TabControl x:Name="Sections_Tabs" Margin="44,72,43,31"  BorderBrush="{x:Null}" Foreground="{x:Null}" Background="{x:Null}" Padding="0,0,0,0" SelectionChanged="Sections_Tabs_SelectionChanged">
                    <TabItem Header="Client Mods" Width="150">
                        <TabItem.BorderBrush>
                            <LinearGradientBrush EndPoint="1,0">
                                <GradientStop Color="#FF326CF3"/>
                                <GradientStop Color="#FF02FFB4" Offset="1"/>
                            </LinearGradientBrush>
                        </TabItem.BorderBrush>
                    </TabItem>
                    <TabItem Header="Skins" Width="150">
                        <TabItem.BorderBrush>
                            <LinearGradientBrush EndPoint="1,0">
                                <GradientStop Color="#FF326CF3"/>
                                <GradientStop Color="#FF02FFB4" Offset="1"/>
                            </LinearGradientBrush>
                        </TabItem.BorderBrush>
                    </TabItem>
                    <TabItem Header="DDS Skins" Width="150">
                        <TabItem.BorderBrush>
                            <LinearGradientBrush EndPoint="1,0">
                                <GradientStop Color="#FF326CF3"/>
                                <GradientStop Color="#FF02FFB4" Offset="1"/>
                            </LinearGradientBrush>
                        </TabItem.BorderBrush>
                    </TabItem>
                    <TabItem Header="Server Mods" Width="150">
                        <TabItem.BorderBrush>
                            <LinearGradientBrush EndPoint="0,1" StartPoint="1,0">
