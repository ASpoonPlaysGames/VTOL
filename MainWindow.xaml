                    <Label Content="{DynamicResource UI_TEXT_MAIN_NORTHSTAR_STATUS}" Height="20" HorizontalAlignment="Left" Background="{x:Null}" BorderBrush="{x:Null}" Padding="0,0,0,0" Width="96" Margin="10,0,0,0">
