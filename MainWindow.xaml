                                    <Image Source="/Resources/Folder_Browse_Icon.png" />
                                </MenuItem.Icon>
                            </MenuItem>
                            <MenuItem x:Name="Delete_Menu_Context" Header="Delete" Click="Delete_Menu_Context_Click" Visibility="Visible" IsEnabled="True">
                                <MenuItem.Icon>
                                    <Image Source="/Resources/Delete_Icon.png" />
                                </MenuItem.Icon>
                            </MenuItem>
                            <MenuItem x:Name="SortMenu" Header="Sort" Visibility="Visible" IsEnabled="True" Click="SortMenu_Click">
                                <MenuItem.Icon>
                                    <Image x:Name="Sort_Img_Source" Source="/Resources/Sort_Off.png" />
