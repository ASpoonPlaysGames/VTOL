                    <hc:Badge x:Name="Indicator_Server_Conn" Content="" Height="13" Padding="3.9,-1,4,0" Margin="118,16,0,16" BorderBrush="White" Background="#FFFFD200" Text="Click To Start" BadgeMargin="7,-7,-3,2" RenderTransformOrigin="1,1" FontSize="16" Status="Processing" BorderThickness="1,1,1,1" SnapsToDevicePixels="True" HorizontalAlignment="Left" Width="17"/>

                    <hc:SimplePanel Margin="134,8,10,8" MouseDown="Label_MouseDown"  >

                        <Ellipse x:Name="Origin_Client_Status" Margin="0,2,8,2" Fill="#FF7DFF00" Width="25" HorizontalAlignment="Right"/>
                        <Label HorizontalAlignment="Left" Background="{x:Null}" BorderBrush="{x:Null}" Padding="0,0,0,0"  Content="{DynamicResource UI_TEXT_MAIN_ORIGIN_STATUS}" Margin="13,4,0,4" VerticalAlignment="Stretch" Width="109"/>
                        <hc:Badge x:Name="Indicator_Origin_Client" Content="" Height="13" Padding="3.9,-1,4,0" Margin="0,7,8,8" BorderBrush="White" Background="#FFFFD200" Text="Click To Start" BadgeMargin="7,-7,-3,2" RenderTransformOrigin="1,1" FontSize="16" Status="Processing" BorderThickness="1,1,1,1" SnapsToDevicePixels="True" HorizontalAlignment="Right" Width="17" Visibility="Hidden"/>
                        <hc:Poptip.Instance>
