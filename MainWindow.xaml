            </ScrollViewer>
            <Image x:Name="Banner_Image" Margin="38,409,37,0" Width="935" Height="203" Source="Resources/Banner_normal.jpg" Stretch="UniformToFill"  />
            <hc:GifImage x:Name="Gif_Image_Vanilla" Height="203" VerticalAlignment="Top" Margin="38,409,37,0" Stretch="Fill" Visibility="Hidden" Panel.ZIndex="3" IsHitTestVisible="False" gif:AnimationBehavior.AutoStart="False" ScrollViewer.VerticalScrollBarVisibility="Disabled">
                <hc:GifImage.Source>
                    <BitmapImage x:Name="Bitmap1" UriSource="\Resources\TF2_Vanilla_promo.gif" CacheOption="OnLoad"/>
                </hc:GifImage.Source>
            </hc:GifImage>
            <hc:GifImage x:Name="Gif_Image_Northstar" Height="203" VerticalAlignment="Top" Margin="38,409,37,0" Stretch="Fill" Visibility="Hidden" HorizontalAlignment="Right" Width="930" Panel.ZIndex="3" gif:AnimationBehavior.AutoStart="False" ScrollViewer.VerticalScrollBarVisibility="Disabled">
                <hc:GifImage.Source>
                    <BitmapImage x:Name="Bitmap2" UriSource="\Resources\NorthStar_Smurfson.gif" CacheOption="OnLoad"/>
                </hc:GifImage.Source>
            </hc:GifImage>
            <Label Content="{DynamicResource UI_TITLE_MAIN}" Height="44" Width="190" Margin="0,6,0,0" VerticalAlignment="Top" Background="{x:Null}" BorderBrush="{x:Null}" FontSize="20" FontWeight="Bold" FontFamily="Gadugi"/>
        </hc:SimplePanel>
        <hc:SimplePanel Background="Black" HorizontalAlignment="Left" Width="1010" Grid.Column="1" VerticalAlignment="Top" ScrollViewer.CanContentScroll="True" Grid.IsSharedSizeScope="True" ClipToBounds="True" Height="104" Grid.Row="1" Margin="0,212,0,0" Grid.RowSpan="2" Grid.ColumnSpan="3">
            <Button x:Name="Titanfall_2_Btn"  Height="54" Width="430" Background=  "{DynamicResource Button_BG}" BorderThickness="1,1,1,1" BorderBrush="{x:Null}" HorizontalAlignment="Left" Margin="35,0,0,0" MouseEnter="Titanfall_2_Btn_MouseEnter" Click="Titanfall_2_Btn_Click"  >
                <StackPanel Orientation="Horizontal">
                    <Image Source="Resources/TF_ICON.png" Width="24" Height="24"/>

                    <TextBlock Text="{DynamicResource UI_BUTTON_MAIN_LAUNCH_TF2}" VerticalAlignment="Center" />
                </StackPanel>
            </Button>
            <Button x:Name="Northstar_Btn" Height="54" Background=  "{DynamicResource Button_BG}" BorderThickness="1,1,1,1" BorderBrush="#FF02FFB4" HorizontalAlignment="Stretch" Margin="524,0,46,0" MouseEnter="Northstar_Btn_MouseEnter" Click="Northstar_Btn_Click" Width="430">
                <StackPanel Orientation="Horizontal">
                    <Image x:Name="Icon_Holder" Source="{DynamicResource Northstar_Icon}" Width="24" Height="24"/>
                    <TextBlock Text="{DynamicResource UI_BUTTON_MAIN_LAUNCH_NS}" VerticalAlignment="Center" />
                </StackPanel>
            </Button>
        </hc:SimplePanel>









