                                <GradientStop Color="#FF326CF3"/>
                                <GradientStop Color="#FF02FFB4" Offset="1"/>
                            </LinearGradientBrush>
                        </TabItem.BorderBrush>
                    </TabItem>
                    <TabItem x:Name="All_Tab" Header="All" Margin="0,0,-76,0">
                        <TabItem.BorderBrush>
